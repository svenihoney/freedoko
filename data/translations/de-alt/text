GameType::solo king-jack		= Königen-Unter-Solo
GameType::solo king-queen		= Königen-Ober-Solo
GameType::solo koehler			= Köhler
GameType::solo club			= Eichel-Solo
GameType::solo spade			= Gras-Solo
GameType::solo heart			= Herz-Solo
GameType::solo diamond			= Schellen-Solo
